<!--カレンダー-->
    <div>
      <%= link_to 'トレーニング開始', new_training_path, class: " btn btn-outline-danger mr-5 mx-auto" %>
      <%= month_calendar events: @trainings do |date, training| %>
      <%= date.strftime("%d") %>
      <% training.each do |training| %>
      <div>
        <%= link_to training.start_time.strftime("★"), training_path(training) %>
      </div>
      <% end %>
      <% end %>
    </div>